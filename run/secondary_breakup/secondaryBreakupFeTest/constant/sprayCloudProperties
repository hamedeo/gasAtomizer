	    flowRateProfile constant 10;

            Cd              constant 0.9; //0.9


// Inner and outer half-cone angles
            thetaInner      constant 0.0;
            thetaOuter      constant 0.0;
            

          
            sizeDistribution
            {
                type        uniform;
                uniformDistribution
                {
                    minValue        0.00017;
                    maxValue        0.00017;
                    d               0.00017;
                    n               1;
                }
            }
          }
            

            
