FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (liquid gas);

liquid
{
    transportModel  Newtonian;
    nu              0;
    rho             1e3;
}

gas
{
    transportModel  Newtonian;
    nu              0;
    rho             1;
}


sigma               0;


